{
    "manifest_version": 2,
    "name": "vite",
    "version": "0.0.4",

    "description": "visual interactive test environment",

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "content_scripts": [
        {
            "matches": ["http://localhost:*/*"],
            "js": ["content.js"]
        }
    ],

    "permissions": [
        "tabs",
        "activeTab",
        "<all_urls>"
    ]
}